/* /etc/bind/named.conf.local
*/

zone "runners.dns" {
        type master;
        file "/etc/bind/for.runners";
        allow-transfer {
          172.17.10.5;
          8.8.8.8; 
        }
 };
zone "1.168.192.in-addr.arpa" {
        type master;
        file "/etc/bind/rev.runners";
        allow-transfer {
          172.17.10.5;
          8.8.8.8;
        }

 };
